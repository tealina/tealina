{
  "name": "tealina",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.mjs",
  "description": "A cli for develop APIs with end to end typesave, document generation",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "gen-types": "tsc src/index.ts --declaration --emitDeclarationOnly --esModuleInterop --outDir dist",
    "build": "unbuild && pnpm gen-types",
    "test": "vitest --dir ./test --run"
  },
  "bin": {
    "tealina": "index.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "author": "Neo Yeo",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "cac": "^6.7.14"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "dependencies": {
    "@tealina/doc-types": "workspace:*",
    "chalk": "^5.2.0",
    "consola": "^3.2.3",
    "fp-lite": "^2.0.0",
    "fs-extra": "^11.1.1",
    "pathe": "^1.1.1"
  },
  "peerDependencies": {
    "typescript": ">=5.1.3"
  }
}