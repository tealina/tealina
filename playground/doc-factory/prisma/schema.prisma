generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id     Int     @id @default(autoincrement())
  /// the email, should be unique
  email  String  @unique
  /// the user name
  name   String?
  skills Skills
}

enum Skills {
  Node
  React
  Typescript
}
